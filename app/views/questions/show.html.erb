<div class="col pb-2">
  <%=link_to 'Back', course_testing_path(@course, @testing), class: 'btn btn-outline-info btn-sm' %>
</div>

<%= t('courses.questions.title') %>: <%=@question.question %><br>
<%= t('courses.questions.content') %>: <%=@question.content %>
<br>
<div>
  <%= t('courses.answers.titles') %>:
  <ul>
    <%@question.answers.each do |answer| %>
      <% if answer.isCorrect %>
        <li>
          <%=answer.answer %><br>
        </li>
      <%end %>
    <%end %>
  </ul>
</div>
