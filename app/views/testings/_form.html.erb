<%= form_with(model:  @testing, url: [@course,@testing]) do |f| %>
  <div class="form-group">
    <%=f.label :title %>
    <%= f.text_field :title, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%=f.label :Description %>
    <%= f.text_field :description, class: 'form-control' %>
  </div>

  <div class="action">
    <%=f.submit class: 'btn btn-success' %>
  </div>

<%end %>