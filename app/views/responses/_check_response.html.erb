<%= form_tag course_testing_question_responses_path(course_id: @course.id, testing_id: @testing.id, question_id: @question.id ) do %>
 <%= @question.question %>
  <% @answers.each do |a| %>
    <br>
    <%= fields_for :response  do |fa| %>
      <%= check_box_tag 'response[]', a.answer  %>
      <%=label_tag a.answer %>
    <%end %>
  <%end %>
  <br>
  <%= submit_tag 'Send', class: 'btn btn-success' %>
<% end %>

